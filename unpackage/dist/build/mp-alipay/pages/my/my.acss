.my-bg{min-height:100vh;background:#f6f6f6}.my-bg .user-info{width:750rpx;background:#3162f5;position:relative;z-index:2;color:#fff;text-align:center;padding:0rpx 35rpx 0;height:300rpx}.my-bg .user-info .head-img,
.my-bg .user-info .img{width:135rpx;height:135rpx;border:3rpx solid #fff;background-size:100%;margin:0 auto 30rpx}.my-bg .user-info .nickName{height:33rpx;font-size:44rpx;font-family:PingFangSC-Medium;font-weight:500;color:#fff}.my-bg .user-info .login-btn{width:200rpx;height:56rpx;line-height:54rpx;border:2rpx solid hsla(0,0%,100%,.6);font-size:26rpx;font-weight:500;color:#fff;text-align:center;margin:0 auto;background:transparent;border-radius:28rpx}.my-bg .list{width:690rpx;margin:0 auto;position:relative;top:-40rpx;z-index:9}.my-bg .list .item{text-align:left;background:#fff;border-radius:4px;margin-bottom:20rpx;padding:35rpx 50rpx;box-sizing:border-box;font-size:33rpx;font-family:PingFangSC-Medium;font-weight:500;color:#333;line-height:40rpx;vertical-align:middle}.my-bg .list .item .left-icon{width:40rpx;height:40rpx;display:inline-block;margin-right:20rpx;vertical-align:middle;position:relative;top:-3rpx}.my-bg .list .item .right-icon{width:24rpx;height:24rpx;vertical-align:middle;float:right;line-height:40rpx;margin-top:8rpx}.my-bg .last-gray{text-align:center;position:fixed;left:0;width:100%;bottom:50rpx;height:20rpx;font-size:20rpx;font-weight:400;color:#c2c2c2}.my-bg .last-gray .line{display:inline;padding:0 18rpx}