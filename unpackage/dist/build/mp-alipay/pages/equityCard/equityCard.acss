.home-bg .pro-btn-bg .buy-btn:disabled{background:rgba(228,38,63,.5)}.home-bg .pro-bg{position:fixed;bottom:0;left:0;right:0;min-height:500rpx;z-index:5;background:#fff;border-radius:40rpx 40rpx 0 0;background:#262525;padding:60rpx 36rpx 0}.home-bg .pro-bg .close-icon{width:50rpx;height:50rpx;position:absolute;right:20rpx;top:20rpx;background:url(/common/images/cuo.png) no-repeat;background-size:contain}.home-bg .pro-bg .title{line-height:48rpx;margin-bottom:16rpx;color:#e8be61;font-size:28rpx}.home-bg .pro-bg .con{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap}.home-bg .pro-bg .con .item{min-width:100rpx;margin:0 24rpx 24rpx 0;border:2rpx solid #e8be61;padding:0 12rpx;line-height:30rpx;height:80rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;font-weight:700;border-radius:16rpx;font-size:28rpx;color:#e8be61}.home-bg .pro-bg .con .item.active{background:#e8be61;color:#262525}.home-bg .pro-bg .p-foot{margin-top:100rpx}.home-bg .my-btn{width:52rpx;height:136rpx;line-height:26rpx;background:-webkit-linear-gradient(top,#fdd080,#c89742);background:linear-gradient(180deg,#fdd080,#c89742);box-shadow:0 8rpx 16rpx 0 rgba(0,0,0,.5),0 4rpx 28rpx 0 rgba(0,0,0,.5);border-radius:6rpx 0 0 6rpx;position:fixed;bottom:142rpx;right:0;padding-left:14rpx;padding-top:18rpx;font-size:24rpx;color:#5b3f0c;font-weight:700}