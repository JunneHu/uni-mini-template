.home-bg{width:750rpx;min-height:100vh;box-sizing:border-box;background:#262525;padding:24rpx 0 100rpx}.home-bg image{width:750rpx;background-size:100%;margin-bottom:34rpx}.home-bg .advantage{padding:0 80rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between;font-size:20rpx;color:rgba(226,193,134,.8);vertical-align:middle;line-height:26rpx;margin-bottom:34rpx}.home-bg .buy-area{background:transparent;position:fixed;z-index:2;left:0;bottom:0;right:0}.home-bg .p-foot,
.home-bg .pro-btn-bg{width:678rpx;margin:16rpx auto;height:100rpx;background:url(/common/images/btn-bg.png) no-repeat;background-size:100%;border-radius:61rpx;overflow:hidden;box-shadow:0 4rpx 12rpx 0 rgba(0,0,0,.5),0 .04rpx .28rpx 0 rgba(0,0,0,.5)}.home-bg .p-foot .price-bg,
