.pay-success{width:750rpx;padding-bottom:180rpx;min-height:100vh;background:#f6f6f6}.pay-success .top{background:#fff;padding:27rpx 32rpx 29rpx;margin-bottom:30rpx}.pay-success .top .succ-icon{width:130rpx;height:130rpx;background:#286aff;border-radius:50%;margin:0 auto 40rpx}.pay-success .top .succ-icon image{width:68rpx;height:52rpx;background-size:contain;margin:42rpx 0 0 31rpx}.pay-success .top .txt{height:38rpx;font-size:40rpx;font-family:PingFangSC-Regular;font-weight:400;color:#333;text-align:center;margin-bottom:74rpx}.pay-success .top .btn-bg{display:-webkit-box;display:-webkit-flex;display:flex;width:686rpx;margin:0 auto 37rpx}.pay-success .top .btn-bg button{width:335rpx;height:88rpx;line-height:88rpx;padding:0;font-size:36rpx}.pay-success .top .btn-bg button.toOrder{margin-right:16rpx;background:#d2daf1;color:#286aff;border:none}.pay-success .top .btn-bg button.toIndex{background:#286aff}.pay-success .more-txt{margin-bottom:33rpx;height:25rpx;font-size:26rpx;font-weight:400;color:#999;text-align:center}.pay-success .more-pro{width:686rpx;height:580rpx;background:#fff;box-shadow:0 2px 10px 0 hsla(0,0%,53.3%,.15);border-radius:4rpx;margin:0 auto;padding:42rpx 32rpx 0;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap}.pay-success .more-pro .item{width:33%;margin-bottom:30rpx}.pay-success .more-pro .item image{width:100rpx;height:100rpx;border-radius:8rpx;display:block;margin:0 auto 27rpx}.pay-success .more-pro .item .name{text-align:center;display:block;height:30rpx;font-size:26rpx;font-weight:500;color:#000;margin-bottom:17rpx;overflow:hidden;line-height:30rpx}.pay-success .more-pro .item .price{text-align:center;display:block;height:24rpx;font-size:26rpx;font-weight:400;color:#ff8b40;line-height:26rpx;margin-bottom:8rpx}.pay-success .more-pro .item .del-price{text-decoration:line-through;text-align:center;height:20rpx;line-height:20rpx;font-size:20rpx;color:#b3b3b3}.pay-success .last-gray{width:690rpx;height:22rpx;font-size:22rpx;font-family:PingFangSC-Regular;font-weight:400;color:#ccc;line-height:26rpx;text-align:center;position:fixed;bottom:36rpx}