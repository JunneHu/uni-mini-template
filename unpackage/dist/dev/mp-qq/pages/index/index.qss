.index-bg {
  width: 750rpx;
  min-height: 100vh;
  background: #f6f6f6 url('https://fulu-mall.oss-cn-hangzhou.aliyuncs.com/9fcbea12815649da864d0f7e29e2d887.png') no-repeat;
  background-size: 100%;
}
.index-bg .swiper,
.index-bg .swiper-item,
.index-bg .swiper-item .img {
  width: 680rpx;
  height: 295rpx;
  border-radius: 10rpx;
  margin: 0 auto 20rpx;
}
.index-bg .onemenu-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  width: 750rpx;
  min-height: 180rpx;
  background: #ffffff;
  padding: 30rpx 35rpx 0;
  margin-bottom: 20rpx;
}
.index-bg .onemenu-list .item {
  width: 146rpx;
  overflow: hidden;
  text-align: left;
  margin-bottom: 34rpx;
}
.index-bg .onemenu-list .item:nth-child(5n) {
  width: 96rpx;
}
.index-bg .onemenu-list .item .img {
  width: 88rpx;
  height: 88rpx;
  overflow: hidden;
  background-size: 100%;
  margin-bottom: 0.1rem;
}
.index-bg .onemenu-list .item .name {
  font-size: 24rpx;
  color: #666;
}
.index-bg .goods-bg {
  width: 750rpx;
  background: #fff;
  border-radius: 32rpx;
  padding: 32rpx 0 0 36rpx;
  margin-bottom: 20rpx;
}
.index-bg .goods-bg .pro-list {
  width: 705rpx;
  margin-bottom: 24rpx;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.index-bg .goods-bg .pro-list .title {
  height: 42rpx;
  line-height: 42rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: #333;
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
}
.index-bg .goods-bg .pro-list .go-more {
  font-size: 24rpx;
  color: #999;
  padding-right: 32rpx;
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
  height: 42rpx;
  line-height: 42rpx;
  text-align: right;
}
.index-bg .goods-bg .pro-list .go-more .img {
  width: 22rpx;
  height: 22rpx;
  background-size: 100%;
  margin-left: 7rpx;
  position: relative;
  top: -2rpx;
  opacity: 0.6;
  vertical-align: middle;
  display: inline-block;
}
.index-bg .goods-bg .list {
  width: 705rpx;
  overflow-x: auto;
  overflow-y: hidden;
  height: 340rpx;
}
.index-bg .goods-bg .list .scroll-view_H {
  height: 340rpx;
}
.index-bg .goods-bg .list .scroll-view-item {
  margin-right: 20rpx;
  width: 316rpx;
  height: 290rpx;
  float: left;
  border-radius: 16rpx;
  border: 2rpx solid #f8f8f8;
  box-sizing: border-box;
}
.index-bg .goods-bg .list .scroll-view-item:last-child {
  margin-right: 0;
}
.index-bg .goods-bg .list .scroll-view-item .pro-img {
  display: block;
  width: 312rpx;
  height: 188rpx;
  border-radius: 16rpx 16rpx 0 0;
}
.index-bg .goods-bg .list .scroll-view-item .info {
  padding-left: 10rpx;
}
.index-bg .goods-bg .list .scroll-view-item .name {
  white-space: pre-wrap;
  font-size: 24rpx;
  font-weight: 500;
  color: #333333;
  text-align: left;
  height: 26rpx;
  line-height: 26rpx;
  width: 210rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 8rpx;
  margin-bottom: 6rpx;
}
.index-bg .goods-bg .list .scroll-view-item .price {
  font-size: 24rpx;
  font-weight: 500;
  height: 28rpx;
  text-align: left;
  color: #fe9a5b;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom: 10rpx;
  font-weight: 700;
}
.index-bg .tabs-bg {
  width: 750rpx;
  background: #ffffff;
  padding: 0 32rpx;
}
.index-bg .tabs-bg .tab-title {
  width: 686rpx;
  overflow-x: auto;
  overflow-y: hidden;
  height: 100rpx;
}
.index-bg .tabs-bg .tab-title .scroll-view_H {
  height: 100rpx;
}
.index-bg .tabs-bg .tab-title .item {
  display: inline-block;
  width: 150rpx;
  margin-right: 30rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  line-height: 90rpx;
  position: relative;
}
.index-bg .tabs-bg .tab-title .item:last-child {
  margin-right: 0;
}
.index-bg .tabs-bg .tab-title .item.active {
  color: #3162F4;
  font-size: 40rpx;
  width: 180rpx;
}
.index-bg .tabs-bg .tab-title .item.active:before {
  content: '';
  position: absolute;
  width: 100%;
  height: 6rpx;
  background: #3162F4;
  top: 80rpx;
  left: 0;
}
.index-bg .tabs-bg .tuijian-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 686rpx;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 14rpx 0 24rpx;
}
.index-bg .tabs-bg .tuijian-list .item {
  width: 214rpx;
  margin-right: 20rpx;
  margin-bottom: 32rpx;
  position: relative;
}
.index-bg .tabs-bg .tuijian-list .item:nth-child(3n) {
  margin-right: 0;
}
.index-bg .tabs-bg .tuijian-list .item .img-bg {
  width: 214rpx;
  height: 172rpx;
  background: #faf9f9;
  margin-bottom: 20rpx;
  text-align: center;
  padding-top: 26rpx;
  border-radius: 16rpx;
}
.index-bg .tabs-bg .tuijian-list .item .img-bg .img {
  width: 120rpx;
  height: 120rpx;
}
.index-bg .tabs-bg .tuijian-list .item .name {
  white-space: pre-wrap;
  font-size: 24rpx;
  font-weight: 500;
  color: #000;
  text-align: left;
  width: 210rpx;
  height: 64rpx;
  line-height: 32rpx;
  margin-top: 14rpx;
  margin-bottom: 8rpx;
  overflow: hidden;
}
.index-bg .tabs-bg .tuijian-list .item .price {
  font-size: 26rpx;
  font-weight: 500;
  height: 30rpx;
  line-height: 30rpx;
  text-align: left;
  color: #fe9a5b;
  overflow: hidden;
  margin-bottom: 8rpx;
  font-weight: 700;
}
.index-bg .tabs-bg .tuijian-list .item .price .small {
  font-size: 20rpx;
}
.index-bg .tabs-bg .tuijian-list .item .del-price {
  text-decoration: line-through;
  text-align: left;
  height: 20rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  color: #b3b3b3;
}
.index-bg .index-bottom {
  text-align: center;
  height: 20rpx;
  font-size: 22rpx;
  color: #666;
  margin: 40rpx 0 60rpx;
}
.index-bg .index-bottom image {
  width: 28rpx;
  height: 28rpx;
  background-size: contain;
  margin: 0 5rpx 0 30rpx;
  vertical-align: middle;
}
.index-bg .index-bottom .grayfont {
  padding: 0 24rpx;
  display: inline-block;
}
.index-bg .last-gray {
  width: 750rpx;
  height: 20rpx;
  font-size: 20rpx;
  font-weight: 400;
  text-align: center;
  color: #c2c2c2;
  padding-bottom: 48rpx;
}
.index-bg .last-gray .line {
  display: inline;
  padding: 0 18rpx;
}

