<view class="card-detail"><view class="top-bg"><view class="h1">{{detail.merEquityCardName}}</view><view class="time">{{"有效期至："+detail.equityCardEndTime}}</view></view><block qq:for="{{detail.productModelList}}" qq:for-item="v" qq:for-index="i"><view class="item"><view class="top"><view class="name font-clamp">{{v.productName+''}}<block qq:if="{{v.useStatus==2}}"><view class="orderno">{{"订单编号："+v.orderNo}}</view></block></view><block qq:if="{{v.useStatus==0}}"><block><block qq:if="{{detail.isUse}}"><button data-event-opts="{{[['tap',[['toUrl',['$0','rechange'],[[['detail.productModelList','',i]]]]]]]}}" class="btn-bg" bindtap="__e">立即兑换</button></block><block qq:else><button class="btn-bg disable" disabled="{{true}}">已过期</button></block></block></block><block qq:else><block><block qq:if="{{v.useStatus==3}}"><button data-event-opts="{{[['tap',[['toUrl',['$0','look'],[[['detail.productModelList','',i]]]]]]]}}" class="btn-bg userd" bindtap="__e">查看卡密</button></block><block qq:if="{{v.useStatus==2}}"><button data-event-opts="{{[['tap',[['toUrl',['$0','look'],[[['detail.productModelList','',i]]]]]]]}}" class="btn-bg userd" bindtap="__e">查看详情</button></block><block qq:if="{{v.useStatus==1}}"><button class="btn-bg userd">兑换中</button></block></block></block></view><view class="bottom">{{detail.equityCardEndTime+" 过期"}}</view></view></block></view>