.get-card {
  width: 100%;
  min-height: 100%;
  position: absolute;
  padding: 32rpx;
  background: #f6f6f6;
}
.get-card .panel {
  margin-bottom: 40rpx;
  overflow: hidden;
}
.get-card .panel image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  float: left;
}
.get-card .panel .title {
  margin-left: 120rpx;
  color: #333;
}
.get-card .codeshow {
  width: 702rpx;
  height: 112rpx;
  background: #fff;
  margin: 0 auto 20rpx;
  padding: 32rpx;
  box-sizing: border-box;
  border-radius: 12rpx;
}
.get-card .codeshow .code {
  font-size: 32rpx;
  color: #333;
  float: left;
  line-height: 48rpx;
  width: 500rpx;
}
.get-card .codeshow .copycode {
  float: right;
  width: 120rpx;
  height: 52rpx;
  border-radius: 72rpx;
  background: #3162F5;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 52rpx;
}

