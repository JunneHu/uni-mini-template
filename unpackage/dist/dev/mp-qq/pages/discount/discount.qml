<view class="list-bg"><view class="left-bg"><block qq:for="{{oneMenu}}" qq:for-item="item" qq:for-index="__i0__" qq:key="id"><view data-event-opts="{{[['tap',[['toSecoudList',['$0'],[[['oneMenu','id',item.id,'id']]]]]]]}}" class="{{[item.active?'item active font-clamp':'item font-clamp']}}" bindtap="__e"><text>{{item.className}}</text></view></block></view><block qq:if="{{secondMenu[0]}}"><view class="right-bg"><block qq:for="{{secondMenu}}" qq:for-item="item" qq:for-index="__i1__" qq:key="childCategoryId"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['secondMenu','childCategoryId',item.childCategoryId,'childCategoryId']]]]]]]}}" class="item" bindtap="__e"><block qq:if="{{item.cornerMark}}"><view class="right-tips"><image class="img" src="https://fulu-mall.oss-cn-hangzhou.aliyuncs.com/8fee7d6f31eb44b19c6bd8917f32661b.png"></image><text>{{item.cornerMark}}</text></view></block><view class="img-bg"><image src="{{item.iconPath}}"></image></view><view class="name">{{item.childCategoryName}}</view><view class="price"><block qq:if="{{item.integral}}"><text>{{item.integral+"积分+"}}</text></block>{{item.price+'元'}}</view><view class="del-price">{{item.faceValue+"元"}}</view></view></block></view></block></view>