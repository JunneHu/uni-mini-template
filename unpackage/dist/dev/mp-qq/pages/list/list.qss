.lists-page {
  width: 750rpx;
  min-height: 100vh;
  padding: 20rpx 35rpx 120rpx;
  background: #f6f6f6;
}
.lists-page .order-detail-box {
  width: 680rpx;
  height: 186rpx;
  background: #fff;
  margin-bottom: 20rpx;
  padding: 32rpx 20rpx;
  border-radius: 8rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.lists-page .order-detail-box .left {
  width: 140rpx;
  height: 116rpx;
  margin-right: 25rpx;
  background: #F9F9F9;
  border-radius: 12rpx;
  text-align: center;
  padding-top: 14rpx;
}
.lists-page .order-detail-box .left image {
  width: 88rpx;
  height: 88rpx;
  background-size: 100%;
  border-radius: 8rpx;
}
.lists-page .order-detail-box .right {
  width: 144rpx;
  height: 50rpx;
  background: #2C60FF;
  color: #fff;
  text-align: center;
  border-radius: 30rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  margin-top: 60rpx;
  padding: 0;
}
.lists-page .order-detail-box .center {
  width: 330rpx;
  height: 100rpx;
}
.lists-page .order-detail-box .center .name {
  font-size: 32rpx;
  margin-bottom: 34rpx;
  width: 380rpx;
  line-height: 34rpx;
  overflow: hidden;
  height: 34rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #333;
}
.lists-page .order-detail-box .center .price {
  font-size: 32rpx;
  color: #FE9A5B;
  padding-left: 5rpx;
  font-weight: 700;
}
.lists-page .order-detail-box .center .price .del-price {
  text-decoration: line-through;
  text-align: center;
  height: 18rpx;
  font-size: 22rpx;
  color: #B7B7B7;
  padding-left: 20rpx;
}
.lists-page .order-detail-box .center .price .small {
  font-size: 20rpx;
}

