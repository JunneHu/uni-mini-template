<view class="lists-page"><block qq:if="{{total>0}}"><view><block qq:for="{{listData}}" qq:for-item="item" qq:for-index="__i0__"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['listData','',__i0__]]]]]]]}}" class="order-detail-box" bindtap="__e"><view class="left"><image src="{{item.iconPath}}"></image></view><view class="center"><view class="name">{{item.productName}}</view><view class="price"><block qq:if="{{item.integral}}"><text>{{item.integral+"积分+"}}</text></block>{{item.price+'元'}}<text class="del-price">{{item.faceValue+"元"}}</text></view></view><button class="right">立即购买</button></view></block></view></block></view>