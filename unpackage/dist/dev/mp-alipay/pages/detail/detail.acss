  margin-bottom: 75rpx;
  min-height: 200rpx;
}
.detail-bg .goods-desc .desc-main .desc-txt rich-text {
  overflow: hidden;
  word-break: break-all;
}
.detail-bg .goods-desc .desc-main .rule-pic {
