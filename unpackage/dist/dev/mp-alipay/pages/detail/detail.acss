  padding: 40rpx 35rpx;
  margin-bottom: 41rpx;
}
.detail-bg .goods-desc .desc-main .h6 {
  height: 25rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
