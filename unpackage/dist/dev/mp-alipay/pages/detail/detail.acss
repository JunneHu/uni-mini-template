}
.detail-bg .goods-info .d-info .right2 {
  width: 240rpx;
  text-align: right;
  color: #fff;
  padding-right: 24rpx;
}
.detail-bg .goods-info .d-info .right2 .tips-bg {
  height: 37rpx;
  line-height: 35rpx;
  text-align: center;
  border: 2rpx solid #fff;
  border-radius: 4rpx;
  display: inline-block;
  margin-bottom: 13rpx;
  color: #fff;
  font-size: 24rpx;
  padding: 0 10rpx;
}
.detail-bg .goods-info .d-info .left {
  width: 367rpx;
}
.detail-bg .goods-info .d-info .left .time {
  width: 367rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.detail-bg .goods-info .d-info .right {
  width: 366rpx;
  color: #fff;
  font-size: 26rpx;
  text-align: right;
  padding-right: 24rpx;
}
.detail-bg .pro-list {
  background: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 30rpx 31rpx 0;
}
.detail-bg .pro-list .item {
  width: 216rpx;
  height: 120rpx;
  border-radius: 8rpx;
  padding: 0 5rpx;
  background: #ebebf2;
  color: #333333;
  font-size: 28rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  border: 2rpx solid #ebebf0;
  text-align: center;
  line-height: 30rpx;
  margin: 0 19rpx 19rpx 0;
  overflow: hidden;
  position: relative;
}
.detail-bg .pro-list .item:nth-child(3n) {
  margin-right: 0;
}
.detail-bg .pro-list .item.active {
  border-color: #3270ff;
  color: #3270ff;
}
.detail-bg .pro-list .item.active .active-img {
  display: block;
}
.detail-bg .pro-list .item .pro-name {
  width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.detail-bg .pro-list .item .active-img {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  bottom: 5rpx;
  right: 5rpx;
  display: none;
}
.detail-bg .goods-desc {
  width: 750rpx;
  padding: 30rpx 32rpx 50rpx;
  background: #fff;
}
.detail-bg .goods-desc .h1 {
  height: 34rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #2d2d2d;
  margin-bottom: 30rpx;
  padding-left: 5rpx;
}
.detail-bg .goods-desc .desc-main {
  background: #ffffff;
  box-shadow: 0px 2px 10px 0px rgba(153, 153, 153, 0.2);
