.detail-bg {
  width: 750rpx;
  padding-bottom: 160rpx;
  min-height: 100vh;
}
.detail-bg .goods-img {
  width: 750rpx;
  height: 450rpx;
  position: relative;
  background: #ededed;
}
.detail-bg .goods-img .img {
  width: 750rpx;
  height: 450rpx;
  background-size: 100%;
}
.detail-bg .goods-info {
  width: 750rpx;
}
.detail-bg .goods-info .blue-bg {
