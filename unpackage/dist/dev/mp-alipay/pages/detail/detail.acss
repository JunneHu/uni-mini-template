  color: #333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 34rpx;
}
.detail-bg .product-modal .page-section .picker .left {
  color: #333;
  font-size: 34rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.detail-bg .product-modal .page-section .picker .right {
  color: #333;
  font-size: 34rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: right;
}
.detail-bg .product-modal .page-section .picker .right .row-arrow {
  width: 32rpx;
  height: 32rpx;
  margin-left: 16rpx;
