}
.detail-bg .goods-info .d-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.detail-bg .goods-info .d-info .left2 {
  width: 493rpx;
}
.detail-bg .goods-info .d-info .left2 .name {
  width: 100%;
