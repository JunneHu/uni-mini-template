}
.detail-bg .goods-desc .link-to {
  width: 686rpx;
  height: 100rpx;
  background: #ffffff;
  box-shadow: 0px 2px 10px 0px rgba(153, 153, 153, 0.2);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.detail-bg .goods-desc .link-to .link {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 32rpx 32rpx 32rpx 79rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #333333;
}
.detail-bg .goods-desc .link-to .link:last-child {
  border-left: 1rpx solid rgba(239, 239, 239, 0.6);
}
.detail-bg .goods-desc .link-to image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.detail-bg .goods-desc .link-to .img-right {
  width: 30rpx;
  height: 30rpx;
}
.detail-bg .goods-desc .link-to .img-left {
  margin-right: 18rpx;
}
.detail-bg .goods-desc .link-to text {
  display: inline-block;
  position: relative;
  top: 3rpx;
}
.detail-bg .btn-area {
  position: fixed;
  width: 750rpx;
  z-index: 99;
  bottom: 0;
  left: 0;
  height: 158rpx;
  padding: 35rpx 32rpx;
  background: #fff;
  box-shadow: 0 -10rpx 40rpx rgba(0, 0, 0, 0.11);
}
.detail-bg .btn-area button {
  height: 88rpx;
  line-height: 84rpx;
  padding: 0;
  border-radius: 8rpx;
  padding: 0 40rpx;
  text-align: center;
}
.detail-bg .btn-area button.time-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.detail-bg .btn-area button.blue-btn {
  background: #286aff;
  color: #fff;
}
.detail-bg .btn-area button.disabled-btn {
  background: #ebeef5;
  color: #3d424d;
}
.detail-bg .btn-area button text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.detail-bg .btn-area button text:first-child {
  text-align: left;
}
.detail-bg .btn-area button text:last-child {
  text-align: right;
}
.detail-bg .product-modal {
  width: 750rpx;
  background: #fff;
